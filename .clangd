CompileFlags:
  Add: 
    - -xc
    - -std=c99
    - -Wall
    - -Wextra
    - -Wpedantic
    - -Wconversion
    - -Wsign-conversion
    - -Wshadow
    - -Wstrict-prototypes
    - -Wmissing-prototypes
    - -Wcast-align
    - -Wcast-qual
    - -Wfloat-equal
    - -Wwrite-strings
    - -Wunreachable-code
    - -Wswitch-enum
    - -Wbad-function-cast
    - -Werror
    - -D_POSIX_C_SOURCE=200112L

